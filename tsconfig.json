{
  "compilerOptions": {
	"target": "es5",
	"module": "esnext",
	"lib": [
	  "es2018",
	  "DOM"
	],
	"noEmitOnError": true,
	"declaration": false,
	"sourceMap": true,
	"outDir": "lib",
	"strict": true,
	"noImplicitAny": true,
	"strictFunctionTypes": true,
	"alwaysStrict": true,
	"moduleResolution": "node",
	"allowSyntheticDefaultImports": true,
	"esModuleInterop": true,
	"inlineSources": true,
	"pretty": true
  }
}
